<?xml version="1.0"?>
<root>
    <item>
        <name>Terminal Home Shortcut</name>
        <identifier>private.term_home</identifier>
        <only>TERMINAL</only>
        <autogen>
            __KeyToKey__
            KeyCode::BRACKET_LEFT, ModifierFlag::OPTION_R,
            KeyCode::A, ModifierFlag::CONTROL_L
        </autogen>
    </item>
    
    <item>
        <name>Terminal End Shortcut</name>
        <identifier>private.term_end</identifier>
        <only>TERMINAL</only>
        <autogen>
            __KeyToKey__
            KeyCode::BRACKET_RIGHT, ModifierFlag::OPTION_R,
            KeyCode::E, ModifierFlag::CONTROL_L
        </autogen>
    </item>
    
    <item>
        <name>Home Shortcut</name>
        <identifier>private.home</identifier>
        <not>TERMINAL</not>
        <autogen>
            __KeyToKey__
            KeyCode::BRACKET_LEFT, ModifierFlag::OPTION_R,
            KeyCode::CURSOR_LEFT, ModifierFlag::COMMAND_L
        </autogen>
    </item>
    
    <item>
        <name>End Shortcut</name>
        <identifier>private.end</identifier>
        <not>TERMINAL</not>
        <autogen>
            __KeyToKey__
            KeyCode::BRACKET_RIGHT, ModifierFlag::OPTION_R,
            KeyCode::CURSOR_RIGHT, ModifierFlag::COMMAND_L
        </autogen>
    </item>

